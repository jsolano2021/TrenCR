@model TrenCR.Models.EstacionRuta

@{
    ViewData["Title"] = "Eliminar";
}

<h1 class="alert alert-primary" role="alert">Eliminar</h1>

<h3 class="alert alert-danger" role="alert">Esta seguro de Eliminar esta Estación de la Ruta?</h3>
<div>
   
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Estado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdEstacionNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEstacionNavigation.Nombre)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdRutaNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdRutaNavigation.Nombre)
        </dd class>
    </dl>
    
    <form asp-action="Delete" >
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> 
        <a asp-action="Index" class="btn btn-outline-warning">Volver</a>
    </form>
</div>
